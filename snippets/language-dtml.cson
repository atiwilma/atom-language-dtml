  # Q
  'Quote':
    'prefix': 'q'
    'body': '<q${1: cite="$2"}>$3</q>$0'
  # R
  'Ruby Base':
    'prefix': 'rb'
    'body': '<rb>$1</rb>$0'
  'Ruby Parenthesis':
    'prefix': 'rp'
    'body': '<rp>$1</rp>$0'
  'Ruby Pronunciation':
