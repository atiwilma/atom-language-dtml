# ---- INLINE DTML START -------------------------------------------------------
# ---- MUSS OBEN NOCHMALS (name, prefix) ANGEGEBEN WERDEN!!! -------------------
# ------------------------------------------------------------------------------
  '<dtml-call>':
    'prefix': 'dtml-call'
    'body': '<dtml-call $1>$2'
    'description': 'Mehr Infos ->'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_call'
  '<dtml-comment>':
    'prefix': 'dtml-com'
    'body': '<dtml-comment>$1</dtml-comment>'
    'description': 'Alle Lines innerhalb dem Comment werden ignoriert. Man sollte statt dem normalen HTML Comment diesen verwenden.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_comment'
  '<dtml-comment> inline':
    'prefix': 'dtml-comin'
    'body': '<dtml-comment>$1</dtml-comment>'
    'description': 'Genau wie <dtml-comment>, nur wird der Kommentar inline gestellt.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_comment'
  '<dtml-elif>':
    'prefix': 'dtml-elif'
    'body': '<dtml-elif ${1:else_if_statement}>$2'
    'description': 'Ein Else-If Statement.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_elif'
  '<dtml-else>':
    'prefix': 'dtml-else'
    'body': '<dtml-else>$1'
    'description': 'Ein Else Statement.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_else'
  '<dtml-if><dtml-else>':
    'prefix': 'dtml-if else'
    'body': '<dtml-if ${1:if_statement}>$2<dtml-else>$3</dtml-if>$4'
    'description': 'Ein IF-Statement mit einem Else-Statement eingebunden.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_else'
  '<dtml-if>':
    'prefix': 'dtml-if'
    'body': '<dtml-if ${1:if_statement}>$2</dtml-if>'
    'description': 'Ein IF-Statement.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_if'
  '<dtml-in>':
    'prefix': 'dtml-in'
    'body': '<dtml-in ${1:array}>$2</dtml-in>'
    'description': 'Eine Schleife in DTML.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/dtml_syntax/dtml_in'
  '<dtml-let>':
    'prefix': 'dtml-let'
