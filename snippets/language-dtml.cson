# ------------------------------------------------------------------------------
'.text.dtml, .text.dtml .meta.scope.between-tag-pair, .text.dtml .stringtext':
  'this()':
    'prefix': 'this'
    'body': 'this().$1'
  'REQUEST[\'\']':
    'prefix': 'req'
    'body': 'REQUEST[\'$1\']'
# SEITEN OBJEKT
# ------------------------------------------------------------------------------
  'filtered_child_nodes(\'\')':
    'prefix': 'filtered_child_nodes'
    'body': 'filtered_child_nodes(${3:[\'${1:1}\', \'${2:2}\']})$4'
    'description': 'Array mit allen (oder spezifischen) Unterelementen, die sichtbar sind.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/cms_objekte/seite/filtered_child_nodes_/index_ger.html'
    'leftLabel': 'Seite >'
  'filtered_tree_nodes(\'id\',\'attribute\',\'direction\')':
    'prefix': 'filtered_tree_nodes'
    'body': 'filtered_tree_nodes(\'${1:id}\',\'${2:attribute}\',\'${3:direction}\')$4'
    'description': 'Array mit allen (oder spezifischen) Unterelementen im kompletten Knotenbaum, die sichtbar sind.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/cms_objekte/seite/filtered_tree_nodes_/index_ger.html'
    'leftLabel': 'Seite >'
  'getId()':
    'prefix': 'getId()'
    'body': 'getId()$1'
    'description': 'Liefert die ID des Objektes.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/cms_objekte/seite/getid_/index_ger.html'
    'leftLabel': 'Seite >'
  'get_body_content()':
    'prefix': 'get_body_content()'
    'body': 'get_body_content()$1'
    'description': 'Die gerenderte Ansicht eines Objektes.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/cms_objekte/seite/get_body_content/index_ger.html'
    'leftLabel': 'Seite >'
  'get_child_by_id(\'id\')':
    'prefix': 'get_child_by_id'
    'body': 'get_child_by_id(\'${1:id}\')'
    'description': 'Objekt der übergebenen ZMS ID.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/cms_objekte/seite/get_child_by_id_/index_ger.html'
    'leftLabel': 'Seite >'
  'get_child_nodes(\'id\')':
    'prefix': 'get_child_nodes'
    'body': 'get_child_nodes(\'${1:id}\')'
    'description': 'Array mit allen (oder spezifischen) Unterelementen.'
    'descriptionMoreURL': 'http://intranet.atikon.at/content/inhalte/dtml_dokumentation/cms_objekte/seite/get_child_nodes_/index_ger.html'
    'leftLabel': 'Seite >'
  'get_title()':
    'prefix': 'get_title'
    'body': 'get_title()$1'
